<?xml version="1.0" encoding="UTF-8"?>
<dmz>
<runtime>
<resource-map>
   <search>
      <path value="../../assets/tank/models"/>
      <path value="../../assets/tank/images"/>
      <path value="./assets/"/>
   </search>
   <search name="audio">
      <path value="../../assets/tank/sounds"/>
      <path value="./assets/"/>
   </search>
   <resource name="DVTE Vehicle">
      <render>
         <articulate>
            <attribute type="scalar" name="DMZ_Turret_1" node="dof_turret_01" axis="y"/>
            <attribute type="scalar" name="DMZ_Gun_1" node="dof_gun_01" axis="x"/>
         </articulate>
      </render>
   </resource>
   <resource name="logo" file="DMZ.png"/>
   <resource name="smoke" file="smoke.rgb"/>
   <resource name="fire" file="fire.rgb"/>
   <resource name="tank arrow" file="tank_arrow.png"/>
   <resource name="digits" file="tank_digits.png"/>
   <resource name="tank kills" file="tank_kills.png"/>
   <resource name="tank hits" file="tank_hits.png"/>
   <resource name="tank deaths" file="tank_deaths.png"/>
   <resource name="crosshairs" file="crosshairs.png"/>
   <resource name="m1a1" file="m1a1.ive">
      <render>
         <articulate template="DVTE Vehicle"/>
      </render>
   </resource>
   <resource name="t72m" file="t72m.ive">
      <render>
         <articulate template="DVTE Vehicle"/>
      </render>
   </resource>
   <resource name="m830" file="round.ive"/>
   <resource name="engine-sound" file="engine.wav" path="audio"/>
   <resource name="collision-sound" file="collision.wav" path="audio"/>
   <resource name="launch-sound" file="launch.wav" path="audio"/>
   <resource name="detonation-sound" file="explosion.wav" path="audio"/>
</resource-map>
</runtime>
</dmz>
